    <xs:attributeGroup ref="tei:tei_att.pointing.attribute.evaluate"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="tei_att.pointing.attribute.targetLang">
    <xs:attribute name="targetLang">
      <xs:annotation>
        <xs:documentation>specifies the language of the content to be found at the destination referenced by target, using a language tag generated according to BCP 47.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:union memberTypes="xs:language">
          <xs:simpleType>
            <xs:restriction base="xs:token">
              <xs:enumeration value=""/>
            </xs:restriction>
          </xs:simpleType>
        </xs:union>
      </xs:simpleType>
    </xs:attribute>
